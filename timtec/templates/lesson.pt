                </div>
            </div>
            
            <div class="track" tal:repeat="block lesson.blocks">
                <i tal:condition="block.activity" class="icon-list icon-white"></i>
                <div class="track-title" tal:condition="block.video" tal:content="block.video.name"></div>
            </div>
        </section>
        
